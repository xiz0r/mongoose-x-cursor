{
  "name": "mongoose-x-cursor",
  "version": "1.0.0",
  "description": "Opinionated mongoose plugin for cursor based pagination",
  "type": "module",
  "scripts": {
    "build": "tsup ./src --external mongoose",
    "test": "vitest --config ./vitest.config.mts",
    "lint": "eslint --ext .ts ./src ./test"
  },
  "keywords": [
    "mongoose",
    "cursor",
    "pagination",
    "performance",
    "mongodb",
    "paginate",
    "paging",
    "next",
    "prev",
    "nextpage",
    "prevpage",
    "total",
    "paginator",
    "plugin"
  ],
  "author": "Juan Colo <juan.colo@typeof.software>",
  "license": "MIT",
  "files": [
    "dist",
    "package.json"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "eslint": "^8.55.0",
    "eslint-config-standard-with-typescript": "^42.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-promise": "^6.0.0",
    "mongodb-memory-server": "^9.1.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.0.4"
  },
  "dependencies": {
    "mongoose": "^8.0.3"
  }
}
