{
  "name": "mongoose-x-cursor",
  "version": "1.0.7",
  "description": "Opinionated mongoose plugin for cursor based pagination",
  "scripts": {
    "build": "tsup ./src --external mongoose --format cjs",
    "test": "vitest --config ./vitest.config.mts",
    "lint": "eslint --ext .ts ./src ./test --fix"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "mongoose",
    "cursor",
    "pagination",
    "performance",
    "mongodb",
    "paginate",
    "paging",
    "next",
    "prev",
    "nextpage",
    "prevpage",
    "total",
    "paginator",
    "plugin"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/xiz0r/mongoose-x-cursor"
  },
  "author": "Juan Colo <juan.colo@typeof.software>",
  "license": "MIT",
  "files": [
    "dist",
    "package.json"
  ],
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "eslint": "^8.55.0",
    "eslint-config-standard-with-typescript": "^42.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-promise": "^6.0.0",
    "mongodb-memory-server": "^9.1.1",
    "tsup": "^8.0.1",
    "typescript": "^4.9.5",
    "vitest": "^1.0.4",
    "mongoose": "^7.0.0"
  },
  "peerDependencies": {
    "mongoose": "^7.0.0 || ^8.0.0"
  }
}
